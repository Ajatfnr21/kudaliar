[
  // Find the dependents for the current module
  {
    "keys": ["super+alt+up"],
    "command": "dependents"
  },

  // Jump to a dependency
  {
    "keys": ["super+alt+right"],
    "command": "jump_to_dependency"
  },

  // Copy path to clipboard
  {
    "keys": ["super+shift+c"],
    "command": "get_path"
  }
]
